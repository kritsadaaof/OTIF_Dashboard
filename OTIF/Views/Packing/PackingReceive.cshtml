
@{
    ViewBag.Title = "Requests";
}

@{
    ViewBag.Title = "Move Page";
}

@Scripts.Render("~/PackingReceive/js")
 
<div class="row text-center">
    <h3>ยิงบาร์โค้ด TAG ล้อเข้า Packing</h3>
    <i style="color:red">(ยิงบาร์โค้ด TAG ล้อเข้า Packing เพื่อประกอบล้อ)</i>
</div> 



<div class="row text-center">  
    <div class="" style="">
        <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12 text-left">
            <div class="form-element-list">
                <input type="time" id="Time_Actual" class="form-control hidden">

                <div class="row">
                    <div class="col-lg-4 col-md-4 col-sm-3 col-xs-2">
                    </div>
                    <div class="col-lg-4 col-md-4 col-sm-6 col-xs-8">
                        <div class="col-lg-1">
                        </div>
                        <div class="col-lg-11">
                            <div class="form-group nk-datapk-ctm form-elet-mg">
                                <i>บาร์โค้ด TAG ล้อ</i><i style="color:red"> **</i><i id="Data_Location" style="color:red"></i>
                                <div class="input-group nk-int-st">
                                    <input id="Bar_Kam" type="text" class="form-control" placeholder="ยิงบาร์โค้ด TAG ล้อ" />
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-lg-4 col-md-4 col-sm-3 col-xs-2">
                    </div>
                </div>
                <div class="row">
                    <div class="col-lg-4 col-md-4 col-sm-3 col-xs-2">
                    </div>
                    <div class="col-lg-4 col-md-4 col-sm-6 col-xs-8">
                        <div class="col-lg-1">
                        </div>
                        <div class="col-lg-11">
                            <div class="form-group nk-datapk-ctm form-elet-mg">
                                <i>ผู้บันทึก</i><i style="color:red"> **</i>
                                <div class="input-group nk-int-st">
                                    <input id="User" type="text" class="form-control" placeholder="กรอกรหัสผู้บันทึก" />
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-lg-4 col-md-4 col-sm-3 col-xs-2">
                    </div>
                </div>

                <div class="row">
                    <div class="col-lg-4 col-md-4 col-sm-3 col-xs-2">
                    </div>
                    <div class="col-lg-4 col-md-4 col-sm-6 col-xs-8">
                        <div class="col-lg-1">
                        </div>
                        <div class="col-lg-11">
                            <div class="form-group nk-datapk-ctm form-elet-mg">
                                <i>Reel No</i><i style="color:red"> **</i><i style="color:red"></i>
                                <div class="input-group nk-int-st">
                                    <input id="ReelNo" type="text" class="form-control" placeholder="ล้อที่" onkeyup="this.value=this.value.replace(/[^0-9.]/g,'')" />
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-lg-4 col-md-4 col-sm-3 col-xs-2">
                    </div>
                </div>
                <div class="row">
                    <div class="col-lg-4 col-md-4 col-sm-3 col-xs-2">
                    </div>
                    <div class="col-lg-4 col-md-4 col-sm-6 col-xs-8">
                        <div class="col-lg-1">
                        </div>
                        <div class="col-lg-11">
                            <div class="form-group nk-datapk-ctm form-elet-mg">
                                <i>น้ำหนักจากตาชั่ง</i><i style="color:red"> **</i><i style="color:red"></i>
                                <div class="input-group nk-int-st">
                                    <input id="Gross" type="text" class="form-control" placeholder="กรอกน้ำหนัก" onkeyup="this.value=this.value.replace(/[^0-9.]/g,'')" />
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-lg-4 col-md-4 col-sm-3 col-xs-2">
                    </div>
                </div>



                <div class="row" hidden>
                    <div class="col-lg-4 col-md-4 col-sm-3 col-xs-2">
                    </div>
                    <div class="col-lg-4 col-md-4 col-sm-6 col-xs-8">
                        <div class="col-lg-1">
                        </div>
                        <div class="col-lg-11">
                            <div class="form-group nk-datapk-ctm form-elet-mg">
                                <i>Location</i><i style="color:red"> ** </i>  <i id="LocationDes" style="color:red"></i>
                                <div class="input-group nk-int-st">
                                    <input id="Location" type="text" class="form-control" placeholder="กรอกข้อมูล Location" />
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-lg-4 col-md-4 col-sm-3 col-xs-2">
                    </div>
                </div>


                <div class="row" id="CheckBox" style="display:none">
                    <div class="fm-checkbox">

                        <div class="col-lg-4 col-md-4 col-sm-3 col-xs-2">
                        </div>
                        <div class="col-lg-4 col-md-4 col-sm-6 col-xs-8">
                            <div class="col-lg-1">
                            </div>
                            <div class="col-lg-11">
                                <label><input id="Packing1" type="checkbox" class=""> <i style="color:darkcyan"> Packing 1 (ใหม่)</i></label>&emsp;
                                <label><input id="Packing2" type="checkbox" class=""> <i style="color:coral"> Packing 2 (เก่า)</i></label>
                            </div>
                        </div>
                        <div class="col-lg-4 col-md-4 col-sm-3 col-xs-2">
                        </div>
                    </div>
                </div>


                <div class="row" id="Show_Detail" style="display:none">
                    <div class="row text-center">
                        <h4 id="Status" style="color:brown"></h4>
                    </div>
                    <div class="col-lg-4 col-md-4 col-sm-3 col-xs-2">
                    </div>
                    <div class="col-lg-3 col-md-3 col-sm-4 col-xs-5">
                        <div class="form-group nk-datapk-ctm form-elet-mg">
                            <i>บาร์โค้ด Kanban</i>
                            <div class="input-group  nk-int-st" style="color:blue">
                                <i>- </i><i id="Bar_Kam_De"></i>
                            </div>
                        </div>
                    </div>
                    <div class="col-lg-4 col-md-4 col-sm-4 col-xs-5">
                        <div class="form-group nk-datapk-ctm form-elet-mg">
                            <i>ความยาว </i>
                            <div class="input-group  nk-int-st" style="color:blue">
                                <i>- </i><i id="QTY_De"></i>
                            </div>
                        </div>
                    </div>
                    <div class="col-lg-1 col-md-1 col-sm-1 col-xs-0">
                    </div>
                </div>


            </div>
        </div>
    </div>
    <div class="row">
        <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12 mg-t-10 text-center">
            <a id="Save" class="btn btn-success notika-btn-success btn-default">บันทึก</a>
            <a href="@Url.Action("Move", new {})" id="Clear" class="btn btn-success notika-btn-success btn-default">Clear</a>
        </div>
    </div>


</div>



